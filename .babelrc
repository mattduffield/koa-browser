{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [ ">0.25%", "not ie 11", "not op_mini all"]
      },
      "forceAllTransforms": true,
      "modules": false,
      "useBuiltIns": true,
      "loose": true,
      "stage-1": true,
      "debug": true
    }]
  ],
  "plugins": [
    "babel-plugin-add-module-exports"
  ]
}