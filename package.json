{
  "name": "@mattduffield/koa-browser",
  "version": "0.0.5",
  "description": "This allows you to run Koa in the browser.",
  "scripts": {
    "build": "webpack --env.build",
    "dev": "webpack --env.dev --progress --colors --watch",
    "build2": "WEBPACK_ENV=build webpack --env.build",
    "dev2": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "url": "git+ssh://git@github.com/mattduffield/koa-browser.git",
    "type": "git"
  },
  "author": "Matt Duffield <matt.duffield@gmail.com> (https://frontendcreator.com/)",
  "license": "MIT",
  "main": "dist/amd/koa-browser.js",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.7.0",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "puppeteer": "^1.1.1",
    "tape": "^4.9.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "content-type": "1.0.4",
    "delegates": "1.0.0",
    "koa-compose": "4.0.0",
    "statuses": "1.4.0"
  }
}
